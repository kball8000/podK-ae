{
    "name": "Podsy",
    "description": "Play audio and video podcasts",
    "version": "0.0.0.1",
    "manifest_version": 2,
    "app": {
        "urls": [
            "*://kball-test-tools.appspot.com/",
            "*://www.kball-test-tools.appspot.com/"
            ],
        "launch": {
            "web_url": "http://kball-test-tools.appspot.com"
            }
        },
    "icons": {
        "192": "/images/launcher-icon-4x.png"
        },
    "permissions": [
		"background",
        "unlimitedStorage",
        "notifications"
        ]
    },
	"background_page": "https://kball-test-tools.appspot.com/background"
}